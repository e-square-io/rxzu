{
  "name": "ngx-diagrams",
  "version": "3.3.4",
  "author": "Daniel Netzer",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.21.1"
  },
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint --fix --linter=eslint",
    "affected:dep-graph": "nx affected:dep-graph",
    "lint:affected": "nx affected:lint --parallel --uncommitted --only-failed",
    "lint:fix": "yarn lint --fix",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "pack-zip": "cd dist/libs/ngx-diagrams && npx bestzip ../ngx-diagrams.zip * && cd ../../",
    "storybook": "yarn docs:json && nx run ngx-diagrams:storybook",
    "docs:json": "compodoc -p ./libs/ngx-diagrams/tsconfig.lib.prod.json -e json -d ./libs/ngx-diagrams",
    "build-storybook": "yarn docs:json && npx nx run ngx-diagrams:build-storybook",
    "run-many": "npx nx run-many --target=build --configuration=production --projects=ngx-diagrams && yarn build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.1",
    "@angular/common": "^11.0.1",
    "@angular/compiler": "^11.0.1",
    "@angular/core": "^11.0.1",
    "@angular/forms": "^11.0.1",
    "@angular/platform-browser": "^11.0.1",
    "@angular/platform-browser-dynamic": "^11.0.1",
    "@angular/router": "^11.0.1",
    "@nrwl/angular": "^11.0.1",
    "core-js": "^3.6.5",
    "dagre": "^0.8.5",
    "pathfinding": "0.4.18",
    "paths-js": "0.4.11",
    "rxjs": "~6.6.3",
    "snyk": "^1.436.0",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "ng-packagr": "11.0.3",
    "@angular-devkit/core": "^11.0.4",
    "@angular/cli": "~11.0.4",
    "@babel/core": "^7.11.5",
    "babel-loader": "^8.0.0",
    "@angular/compiler-cli": "~11.0.1",
    "@angular/language-service": "~11.0.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@nrwl/cli": "^11.0.1",
    "@nrwl/cypress": "^11.0.1",
    "@nrwl/eslint-plugin-nx": "^11.0.1",
    "@nrwl/jest": "^11.0.1",
    "@nrwl/storybook": "^11.0.1",
    "@nrwl/tao": "^11.0.1",
    "@nrwl/workspace": "^11.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.8",
    "@storybook/angular": "^6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@types/dagre": "^0.7.44",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "@types/pathfinding": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "cypress": "^5.5.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "git-cz": "^4.7.4",
    "husky": "^4.3.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "prettier": "2.1.2",
    "semantic-release": "^17.3.0",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "typescript": "~4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "snyk": true
}
