{
  "version": 1,
  "projects": {
    "angular": {
      "projectType": "library",
      "root": "libs/angular",
      "sourceRoot": "libs/angular/src",
      "prefix": "ngx-diagrams",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "tsConfig": ["libs/angular/tsconfig.lib.json", "libs/angular/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!libs/angular/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/angular"],
          "options": {
            "jestConfig": "libs/angular/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "libs/angular/tsconfig.lib.json",
            "project": "libs/angular/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/angular/tsconfig.lib.prod.json"
            }
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/angular/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/angular",
            "config": {
              "configFolder": "libs/angular/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ngx-diagrams-angular-e2e": {
      "root": "apps/ngx-diagrams-angular-e2e",
      "sourceRoot": "apps/ngx-diagrams-angular-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngx-diagrams-angular-e2e/cypress.json",
            "tsConfig": "apps/ngx-diagrams-angular-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngx-diagrams:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ngx-diagrams:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "tsConfig": ["apps/ngx-diagrams-angular-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/ngx-diagrams-angular-e2e/**"]
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "tsConfig": ["libs/core/tsconfig.lib.json", "libs/core/tsconfig.spec.json"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "packageJson": "libs/core/package.json",
            "main": "libs/core/src/index.ts",
            "assets": ["libs/core/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "e315c558-e160-427f-a22e-090f79424ec8"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "core"
}
